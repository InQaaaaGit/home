{{>theme_learningzone/blocks}}

<!-- Banner Section Start -->
<section id="banner-wrapper">
   <div class="callbacks_container">
      <ul class="rslides" id="slider3">
         <li>
            <img src="{{{banner1image}}}" alt="First Slider">
            <div class="container-fluid">
               <div class="caption col-lg-4">
                 {{#bannerheading}}
                  <h1>{{{bannerheading}}}</h1>
                 {{/bannerheading}}
                 {{#bannertagline}}
                  <p>{{{bannertagline}}}</p>
                 {{/bannertagline}} 
                 {{#bannerbuttontext}}

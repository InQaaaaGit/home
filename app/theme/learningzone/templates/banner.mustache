                 {{/bannerheading}}
                 {{#bannertagline}}
                  <p>{{{bannertagline}}}</p>
                 {{/bannertagline}} 
                 {{#bannerbuttontext}}
                  <a class="btn btn-primary" href="{{{bannerbuttonurl}}}">{{{bannerbuttontext}}}</a>
                 {{/bannerbuttontext}} 
               </div>
            </div>
         </li>
         <li>
            <img src="{{{banner3image}}}" alt="Third Slider">
            <div class="container-fluid">
               <div class="caption col-lg-4">
                 {{#bannerheading}}
                  <h1>{{{bannerheading}}}</h1>
                 {{/bannerheading}}
                 {{#bannertagline}}

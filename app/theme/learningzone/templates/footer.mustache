<!-- Footer Section Start -->
{{#displayfootersection}}
<footer id="page-footer">
   <div class="container-fluid">
      <div class="row-fluid">
         <div class="col-lg-3 col-md-6 col-sm-12 footersection">
            {{#footersection1heading}}
               <h5>{{{footersection1heading}}}</h5>
            {{/footersection1heading}}
            <ul>
               {{#footersection1content}}
                  <li>{{{footersection1content}}}</li>
               {{/footersection1content}}
               {{#footersection1email}}
                  <li><a href="mailto:{{{footersection1email}}}">{{{footersection1email}}}</a></li>
               {{/footersection1email}}
               {{#footersection1contactno}}
                  <li>{{{footersection1contactno}}}</li>
               {{/footersection1contactno}}
               {{#footersection1address}}
                  <li>{{{footersection1address}}}</li>
               {{/footersection1address}}
            </ul>
         </div>
         <div class="col-lg-3 col-md-6 col-sm-12 footersection">
            <ul class="footer-link">
               <li>
                  {{#footersection2heading}}
                  <h5>{{{footersection2heading}}}</h5>
                  {{/footersection2heading}}
                  <ul class="common">
                     {{#footersection2link1}}

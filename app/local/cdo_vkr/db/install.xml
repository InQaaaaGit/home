<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/cdo_vkr/db" VERSION="20230228" COMMENT="CDO_VKR таблица хранения данных о загруженных файлах в ВКР"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_cdo_vkr_file_of_vkr" COMMENT="CDO VKR Таблица хранения данных о файлах в ВКР">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="id_vkr" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="file_id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="type" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="status" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="commentary" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="" DEFAULT=""/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="usermodified" TYPE="foreign" FIELDS="usermodified" REFTABLE="user" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="local_cdo_vkr_vkrs" COMMENT="Список ВКР">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="fio" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="Фамилия Имя Отчество"/>
        <FIELD NAME="name_of_vkr" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="Наименование темы ВКР"/>
        <FIELD NAME="manager" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="Руководитель ВКР"/>
        <FIELD NAME="years" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="учебный год"/>
        <FIELD NAME="edu_group" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Учебная группа студента"/>
        <FIELD NAME="edu_division" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="подразделение студента"/>
        <FIELD NAME="edu_level" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="уровень подготовки"/>
        <FIELD NAME="edu_form" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="форма обучения"/>
        <FIELD NAME="grade" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="оценка за работу"/>
        <FIELD NAME="edu_speciality" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="специальность"/>
        <FIELD NAME="status_id" TYPE="int" LENGTH="11" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="status_changed" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="agreedebs" TYPE="int" LENGTH="1" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="согласия на обработку данных для ЭБС"/>
        <FIELD NAME="acquainted" TYPE="int" LENGTH="1" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="согласия на обработку данных для ЭБС"/>
        <FIELD NAME="user_id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" COMMENT="идентификатор пользователя"/>
        <FIELD NAME="manager_id" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" COMMENT="идентификатор руководителя"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="usermodified" TYPE="foreign" FIELDS="usermodified" REFTABLE="user" REFFIELDS="id"/>
        <KEY NAME="user_id_user" TYPE="foreign" FIELDS="user_id" REFTABLE="user" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>

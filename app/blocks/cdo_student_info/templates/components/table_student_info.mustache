<table class="table table-striped">
    <tbody>
    <tr>
        <td>{{#str}} representation, block_cdo_student_info {{/str}}</td>
        <td>{{presentation}}</td>
    </tr>
    <tr>
        <td>{{#str}} birthday, block_cdo_student_info {{/str}}</td>
        <td>{{birth_date}}</td>
    </tr>
    <tr>
        <td>{{#str}} dormitory, block_cdo_student_info {{/str}}</td>
        <td>{{dormitory.name}} {{dormitory.room}}</td>
    </tr>
    <tr>
        <td>{{#str}} email, block_cdo_student_info {{/str}}</td>
        <td>{{email}}</td>
    </tr>
    <tr>
        <td>{{#str}} phone, block_cdo_student_info {{/str}}</td>
        <td>
            {{#phone}}
                {{.}} <br />
            {{/phone}}</td>
    </tr>
    <tr>
        <td>{{#str}} city, block_cdo_student_info {{/str}}</td>
        <td>{{city}}</td>
    </tr>
    </tbody>
</table>